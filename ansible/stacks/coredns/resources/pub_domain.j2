$TTL 60
$ORIGIN {{ domains_tld }}.
@                   IN	SOA sns.dns.{{ domains_tld }}. noc.dns.{{ domains_tld }}. (
          2017042745 ; serial
          7200       ; refresh (2 hours)				
          3600       ; retry (1 hour)			
          1209600    ; expire (2 weeks)				
          3600       ; minimum (1 hour)				
          )
                         IN NS  ns1.testservice.in. 
@                        IN A   172.21.5.226
*.{{ domains_tld }}.     IN A   172.21.5.226
ns1                      IN A   172.21.5.226

